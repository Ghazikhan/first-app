<p id="notice"><%= notice %></p>

<h1>Listing Degres</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Drege type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @degres.each do |degre| %>
      <tr>
        <td><%= degre.name %></td>
        <td><%= degre.drege_type %></td>
        <td><%= link_to 'Show', degre %></td>
        <td><%= link_to 'Edit', edit_degre_path(degre) %></td>
        <td><%= link_to 'Destroy', degre, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Degre', new_degre_path %>
